{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": { 
          "Access-Control-Allow-Origin": "https://movie-frontend-eight.vercel.app", 
          "Access-Control-Allow-Credentials": "true" 
        }, 
        "dest": "/server.js"
      }
    ]
  }
  